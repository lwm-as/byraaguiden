{
  "enable": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "trailingComma": "none",
  "bracketSpacing": true,
  "disableLanguages": [],
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "ignorePath": ".prettierignore",
  "insertPragma": false,
  "requirePragma": false,
  "jsxBracketSameLine": false,
  "packageManager": "yarn",
  "printWidth": 120,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "resolveGlobalModules": false,
  "useEditorConfig": false,
  "useTabs": false,
  "withNodeModules": false
}
